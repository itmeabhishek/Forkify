s=\"recipe__item\">\r\n        <svg class=\"recipe__icon\">\r\n            <use href=\"img/icons.svg#icon-check\"></use>\r\n        </svg>\r\n        <div class=\"recipe__count\">${formatCount(ingredients.count)}</div>\r\n        <div class=\"recipe__ingredient\">\r\n            <span class=\"recipe__unit\">${ingredients.unit}</span>\r\n            ${ingredients.ingredient}\r\n        </div>\r\n    </li>\r\n`;\r\nexport const renderRecipe = (recipe,isLiked) => {\r\n    const markup = `\r\n        <figure class=\"recipe__fig\">\r\n            <img src=\"${recipe.img}\" alt=\"${recipe.title}\" class=\"recipe__img\">\r\n            <h1 class=\"recipe__title\">\r\n                <span>${recipe.title}</span>\r\n            </h1>\r\n        </figure>\r\n\r\n        <div class=\"recipe__details\">\r\n            <div class=\"recipe__info\">\r\n                <svg class=\"recipe__info-icon\">\r\n                    <use href=\"img/icons.svg#icon-stopwatch\"></use>\r\n                </svg>\r\n                <span class=\"recipe__info-data recipe__info-data--minutes\">${recipe.time}</span>\r\n                <span class=\"recipe__info-text\"> minutes</span>\r\n            </div>\r\n            <div class=\"recipe__info\">\r\n                <svg class=\"recipe__info-icon\">\r\n                    <use href=\"img/icons.svg#icon-man\"></use>\r\n                </svg>\r\n                <span class=\"recipe__info-data recipe__info-data--people\">${recipe.servings}</span>\r\n                <span class=\"recipe__info-text\"> servings</span>\r\n\r\n                <div class=\"recipe__info-buttons\">\r\n                    <button class=\"btn-tiny btn-decrease\">\r\n                        <svg>\r\n                            <use href=\"img/icons.svg#icon-circle-with-minus\"></use>\r\n                        </svg>\r\n                    </button>\r\n                    <button class=\"btn-tiny btn-increase\">\r\n                        <svg>\r\n                            <use href=\"img/icons.svg#icon-circle-with-plus\"></use>\r\n                        </svg>\r\n                    </button>\r\n                </div>\r\n\r\n            </div>\r\n            <button class=\"recipe__love\">\r\n                <svg class=\"header__likes\">\r\n                \r\n                    <use href=\"img/icons.svg#icon-heart${isLiked?'':'-outlined'}\"></use>\r\n                </svg>\r\n            </button>\r\n        </div>\r\n\r\n        <div class=\"recipe__ingredients\">\r\n            <ul class=\"recipe__ingredient-list\">\r\n                ${recipe.ingredients.map(el => createIngredient(el)).join('')}\r\n            </ul>\r\n\r\n            <button class=\"btn-small recipe__btn recipe__btn--add\">\r\n                <svg class=\"search__icon\">\r\n                    <use href=\"img/icons.svg#icon-shopping-cart\"></use>\r\n                </svg>\r\n                <span>Add to shopping list</span>\r\n            </button>\r\n        </div>\r\n\r\n        <div class=\"recipe__directions\">\r\n            <h2 class=\"heading-2\">How to cook it</h2>\r\n            <p class=\"recipe__directions-text\">\r\n                This recipe was carefully designed and tested by\r\n                <span class=\"recipe__by\">${recipe.author}</span>. Please check out directions at their website.\r\n            </p>\r\n            <a class=\"btn-small recipe__btn\" href=\"${recipe.url}\" target=\"_blank\">\r\n                <span>Directions</span>\r\n                <svg class=\"search__icon\">\r\n                    <use href=\"img/icons.svg#icon-triangle-right\"></use>\r\n                </svg>\r\n\r\n            </a>\r\n        </div>\r\n    `;\r\n    elements.recipe.insertAdjacentHTML('afterbegin', markup);\r\n};\r\nexport const updateServingsIngredients=recipe=>{\r\n    //update servings\r\n    document.querySelector('.recipe__info-data--people').textContent=recipe.serving;\r\n    //update ingredients\r\n    const countElements=Array.from(document.querySelectorAll('.recipe__count'));\r\n    countElements.forEach((el,i)=>{\r\n        el.textContent = formatCount(recipe.ingredients[i].count);\r\n    });\r\n    };"},"lineCount":null}},"error":null,"hash":"7b2046c5ce30f5f529b6bf523ffdb9c7","cacheData":{"env":{}}}